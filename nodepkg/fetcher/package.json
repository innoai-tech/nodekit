{
  "name": "@innoai-tech/fetcher",
  "version": "0.5.3",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:^"
  },
  "peerDependencies": {
    "rxjs": "7.x"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:morlay/bunpublish.git",
    "directory": "nodepkg/fetcher"
  },
  "scripts": {
    "build": "bunx --bun monobundle",
    "prepublishOnly": "bun run build",
    "lint": "bunx --bun prettier --write ."
  },
  "sideEffects": false,
  "type": "module"
}
