{
  "name": "@innoai-tech/config",
  "version": "0.4.11",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "./loader": "./src/loader/index.ts",
      "./vite-plugin-inject-config": "./src/vite-plugin-inject-config/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:*",
    "esbuild": "^0.15.5",
    "vite": "^3.0.8"
  },
  "browserslist": [
    "defaults"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    },
    "./loader": {
      "import": {
        "types": "./loader.d.ts",
        "default": "./loader.mjs"
      }
    },
    "./vite-plugin-inject-config": {
      "import": {
        "types": "./vite-plugin-inject-config.d.ts",
        "default": "./vite-plugin-inject-config.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodepkg/config"
  },
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run",
    "prepublishOnly": "pnpm run build",
    "lint": "rome check --apply-suggested ./src && rome format --write ./src"
  },
  "type": "module"
}