{
  "name": "@innoai-tech/reactutil",
  "version": "0.3.1",
  "license": "MIT",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/fetcher": "workspace:*",
    "@innoai-tech/lodash": "workspace:*"
  },
  "peerDependencies": {
    "rxjs": "7.x",
    "react": "18.x"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run",
    "prepublishOnly": "monobundle",
    "lint": "rome check --apply-suggested ./src && rome format --write ./src"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodepkg/reactutil"
  },
  "type": "module",
  "types": "./index.d.ts"
}
