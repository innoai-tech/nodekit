{
  "name": "@innoai-tech/bunpublish",
  "version": "0.1.3",
  "monobundle": {
    "engine": "bun",
    "exports": {
      ".": "./src/index.ts",
      "bin:bunpublish": "./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@npmcli/arborist": "^7.2.0",
    "libnpmpublish": "^9.0.1",
    "pacote": "^17.0.4",
    "globby": "^13.2.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/pacote": "^11.1.6",
    "@types/libnpmpublish": "^4.0.4"
  },
  "bin": {
    "bunpublish": "./bunpublish.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "bunx --bun prettier --write .",
    "build": "bunx --bun monobundle",
    "prepublishOnly": "bun run build"
  },
  "type": "module"
}
