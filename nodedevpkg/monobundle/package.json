{
  "name": "@innoai-tech/monobundle",
  "version": "0.8.7",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "bin:monobundle": "./src/bin/index.ts"
    },
    "pipeline": {
      "build": "tsx ./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:*",
    "@innoai-tech/purebundle": "workspace:*",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "browserslist": "^4.21.5",
    "chalk": "^5.2.0",
    "globby": "^13.1.3",
    "js-yaml": "^4.1.0",
    "minimatch": "^9.0.0",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "yargs": "^17.7.1"
  },
  "peerDependencies": {
    "@swc/core": "^1.3.49",
    "esbuild": "^0.17.12",
    "typescript": "^5.0.x"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/yargs": "^17.0.24"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "prettier --write .",
    "build": "tsx ./src/bin/index.ts",
    "prepublishOnly": "pnpm run build",
    "test": "vitest --run --passWithNoTests --dir ."
  },
  "type": "module"
}