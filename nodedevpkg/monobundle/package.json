{
  "name": "@innoai-tech/monobundle",
  "version": "0.2.0",
  "monobundle": {
    "build": {
      "script": "node --experimental-specifier-resolution=node --loader=ts-node/esm ./src/bin/index.ts"
    },
    "bin": {
      "monobundle": "./src/bin/index.ts"
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "*",
    "@babel/runtime-corejs3": "*",
    "@babel/types": "^7.18.10",
    "@innoai-tech/lodash": "workspace:^*",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "chalk": "^5.0.1",
    "del": "^7.0.0",
    "minimatch": "^5.1.0",
    "rollup": "^2.77.3",
    "rollup-plugin-dts": "^4.2.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@types/node": "*",
    "@types/yargs": "^17.0.11",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "scripts": {
    "build": "node --experimental-specifier-resolution=node --loader=ts-node/esm ./src/bin/index.ts"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodedevpkg/monobundle"
  },
  "type": "module",
  "types": "./index.d.ts"
}
