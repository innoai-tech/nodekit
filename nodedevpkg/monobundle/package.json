{
  "name": "@innoai-tech/monobundle",
  "version": "0.4.3",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "bin:monobundle": "./src/bin/index.ts"
    },
    "pipeline": {
      "build": "tsx ./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:^*",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "browserslist": "*",
    "chalk": "^5.0.1",
    "globby": "^13.1.2",
    "js-yaml": "^4.1.0",
    "minimatch": "^5.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "*",
    "@types/yargs": "^17.0.13",
    "esbuild": "^0.15.9",
    "typescript": "^4.8.3"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodedevpkg/monobundle"
  },
  "scripts": {
    "lint": "rome check --apply-suggested ./src && rome format --write ./src",
    "build": "tsx ./src/bin/index.ts",
    "prepublishOnly": "pnpm run build"
  },
  "type": "module"
}