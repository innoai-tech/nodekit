{
  "name": "@innoai-tech/monobundle",
  "version": "0.7.0",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "bin:monobundle": "./src/bin/index.ts"
    },
    "pipeline": {
      "build": "tsx ./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:*",
    "@innoai-tech/purebundle": "workspace:*",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "browserslist": "^4.21.5",
    "chalk": "^5.2.0",
    "globby": "^13.1.3",
    "js-yaml": "^4.1.0",
    "minimatch": "^7.4.2",
    "rollup": "^3.20.0",
    "rollup-plugin-dts": "^5.3.0",
    "yargs": "^17.7.1"
  },
  "peerDependencies": {
    "esbuild": "^0.17.12",
    "typescript": "^5.0.x",
    "@swc/core": "^1.3.x"
  },
  "devDependencies": {
    "@swc/core": "^1.3.41",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.3",
    "@types/yargs": "^17.0.22"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodedevpkg/monobundle"
  },
  "scripts": {
    "lint": "prettier --write .",
    "build": "tsx ./src/bin/index.ts",
    "prepublishOnly": "pnpm run build",
    "test": "vitest --run --passWithNoTests"
  },
  "type": "module"
}