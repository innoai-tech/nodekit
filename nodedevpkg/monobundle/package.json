{
  "name": "@innoai-tech/monobundle",
  "version": "0.5.0",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "bin:monobundle": "./src/bin/index.ts"
    },
    "pipeline": {
      "build": "tsx ./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:^*",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "browserslist": "*",
    "chalk": "^5.1.2",
    "globby": "^13.1.2",
    "js-yaml": "^4.1.0",
    "minimatch": "^5.1.0",
    "rollup": "^3.3.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.19",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "*",
    "@types/yargs": "^17.0.13",
    "esbuild": "^0.15.14",
    "typescript": "^4.9.3"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodedevpkg/monobundle"
  },
  "scripts": {
    "lint": "prettier --write .",
    "build": "tsx ./src/bin/index.ts",
    "prepublishOnly": "pnpm run build",
    "test": "vitest --run --passWithNoTests"
  },
  "type": "module"
}
