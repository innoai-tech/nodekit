{
  "name": "@innoai-tech/monobundle",
  "version": "0.12.0",
  "monobundle": {
    "exports": {
      ".": "./src/index.ts",
      "bin:monobundle": "./src/bin/index.ts"
    },
    "pipeline": {
      "build": "bun ./src/bin/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/lodash": "workspace:^",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "browserslist": "^4.22.2",
    "chalk": "^5.3.0",
    "esbuild": "^0.19.11",
    "globby": "^14.0.0",
    "js-yaml": "^4.1.0",
    "minimatch": "^9.0.3",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.x"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/yargs": "^17.0.32"
  },
  "bin": {
    "monobundle": "./monobundle.mjs"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts",
    "src/*",
    "!/**/__tests__"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:morlay/bunpublish.git",
    "directory": "nodedevpkg/monobundle"
  },
  "scripts": {
    "lint": "bunx --bun @biomejs/biome check --apply .",
    "build": "bun ./src/bin/index.ts",
    "prepublishOnly": "bun run build"
  },
  "type": "module"
}
