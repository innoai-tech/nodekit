{
  "name": "@innoai-tech/gents",
  "version": "0.2.1",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "node-fetch": "^3.2.10",
    "@innoai-tech/lodash": "workspace:*",
    "@innoai-tech/config": "workspace:*"
  },
  "devDependencies": {
    "@innoai-tech/fetcher": "workspace:*"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:innoai-tech/nodekit.git",
    "directory": "nodedevpkg/gents"
  },
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run",
    "prepublishOnly": "pnpm run build",
    "lint": "rome check --apply-suggested ./src && rome format --write ./src"
  },
  "type": "module"
}