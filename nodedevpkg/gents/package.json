{
  "name": "@innoai-tech/gents",
  "version": "0.4.0",
  "monobundle": {
    "build": {
      "clean": true
    },
    "exports": {
      ".": "./src/index.ts"
    }
  },
  "dependencies": {
    "@innoai-tech/config": "workspace:*",
    "@innoai-tech/lodash": "workspace:*",
    "node-fetch": "^3.3.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@innoai-tech/fetcher": "workspace:*"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.mjs"
      }
    }
  },
  "files": [
    "*.mjs",
    "*.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "build": "monobundle",
    "test": "vitest --run --passWithNoTests --dir .",
    "prepublishOnly": "pnpm run build",
    "lint": "prettier --write ."
  },
  "type": "module"
}