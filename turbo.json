{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "src/*",
        "packages.json"
      ],
      "outputs": [
        "*.d.ts",
        "*.mjs"
      ]
    }
  }
}